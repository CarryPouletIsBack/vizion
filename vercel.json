{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/app-background.png",
      "destination": "/app-background.png"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
