{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/app-background.png",
      "destination": "/app-background.png"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
