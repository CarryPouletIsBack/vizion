{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/app-background.png",
      "destination": "/app-background.png"
    },
    {
      "source": "/(.*\\.(js|css|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "destination": "/$1",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
